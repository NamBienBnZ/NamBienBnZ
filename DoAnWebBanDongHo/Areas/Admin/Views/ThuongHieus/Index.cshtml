@model IEnumerable<DoAnWebBanDongHo.Models.THUONGHIEU>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Thương Hiệu</h2>

<p>
    @Html.ActionLink("Thêm mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Tên thương hiệu
        </th>
        <th>
            Hình thương hiệu
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TENTH)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HINHTH)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.MATH }) |
            @Html.ActionLink("Chi Tiết", "Details", new { id=item.MATH }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.MATH })
        </td>
    </tr>
}

</table>
